{
  "name": "@josephuspaye/win-audio-outputs",
  "version": "0.1.0",
  "description": "List and control system audio outputs from Node.js on Windows",
  "main": "dist/main.js",
  "repository": "https://github.com/JosephusPaye/win-audio-outputs.git",
  "author": "Josephus Paye II",
  "license": "MIT",
  "os": [
    "win32"
  ],
  "engines": {
    "node": ">=10"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"*.{js,json,md}\"",
    "format:check": "prettier --list-different \"src/**/*.ts\" \"*.{js,json,md}\"",
    "test": "echo nah"
  },
  "devDependencies": {
    "@types/node": "^14.14.10",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true
  }
}
